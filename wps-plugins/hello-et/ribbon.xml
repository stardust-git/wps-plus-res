    <command idMso="FileSaveAsOtherFormats" getEnabled="OnGetEnabledSystemTab"/>
    <command idMso="FileSaveAsPDF" getEnabled="OnGetEnabledSystemTab"/>
    <command idMso="FileSaveAsPdfOrXps" getEnabled="OnGetEnabledSystemTab"/>
