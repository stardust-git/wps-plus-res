{
  "scripts": {
    "pc:build": "cross-env mode=prod max build",
    "pc:dev": "cross-env mode=dev max dev",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "cross-env TIMING=1 eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "analyze": "cross-env mode=dev ANALYZE=1 umi build"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@ant-design/pro-components": "^2.7.10",
    "ahooks": "^3.7.4",
    "antd": "4",
    "axios": "^1.4.0",
    "classnames": "2.x",
    "lodash": "^4.17.21",
    "merge-images": "^2.0.0",
    "mockjs": "^1.1.0",
    "moment": "^2.29.4",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.4.10",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@umijs/max": "4.x",
    "cross-env": "^7.0.3",
    "eslint": "7.16.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "stylelint": "^13.13.1",
    "typescript": "^4.1.2",
    "wps-jsapi": "^1.0.5",
    "yorkie": "^2.0.0"
  }
}
